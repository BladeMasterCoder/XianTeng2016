<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.edu.nju.iip.model.BZXX" table="BZXX">
		<meta attribute="class-description">
		This class contains the nju.iip.model.BZXX
			detail.
		</meta>
		<id name="id"  column="BZXXID" type="string">
			<generator class="uuid.hex" />
		</id>
		<property name="corp_Id" column="YHID" type="string" />
		<property name="title" column="BZMC" type="string" />
		<property name="unit" column="BZDW" type="string" />
		<property name="cDate" column="CDATE" type="timestamp" />
		<property name="pDate" column="BZSJ" type="timestamp" />
		<property name="data_Source" column="DATA_SOURCE" type="string" />
	</class>
</hibernate-mapping>