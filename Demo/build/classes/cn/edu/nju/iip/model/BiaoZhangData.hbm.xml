<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.edu.nju.iip.model.BiaoZhangData" table="JW_BiaoZhang">
		<meta attribute="class-description">
			This class contains the nju.iip.model.JudgeDoc detail.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="title" column="Title" type="string" />
		<property name="code" column="Code" type="string" />
		<property name="year" column="Year" type="string" />
		<property name="unit" column="Unit" type="string" />
		<property name="date" column="Date" type="string" />
		<property name="data_Source" column="Data_Source" type="string" />
		<property name="corp_Name" column="Corp_Name" type="string" />
	</class>
</hibernate-mapping>