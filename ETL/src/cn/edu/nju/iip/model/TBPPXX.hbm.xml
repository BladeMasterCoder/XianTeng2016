<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.edu.nju.iip.model.TBPPXX" table="TBPPXX">
		<meta attribute="class-description">
		This class contains the nju.iip.model.TBPPXX
			detail.
		</meta>
		<id name="id"  column="TSXXID" type="string">
			<generator class="uuid.hex" />
		</id>
		<property name="corp_Id" column="YHID" type="string" />
		<property name="content" column="TBNR" type="string" />
		<property name="unit" column="TBBM" type="string" />
		<property name="cDate" column="CDATE" type="timestamp" />
		<property name="uDate" column="UDATE" type="timestamp" />
		<property name="pDate" column="TBRQ" type="string" />
		<property name="data_Source" column="DATA_SOURCE" type="string" />
		<property name="fileName" column="FILENAME" type="string" />
	</class>
</hibernate-mapping>