<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.edu.nju.iip.model.JLXX" table="JLXX">
		<meta attribute="class-description">
		This class contains the nju.iip.model.JLXX
			detail.
		</meta>
		<id name="id"  column="JLXXID" type="string">
			<generator class="uuid.hex" />
		</id>
		<property name="corp_Id" column="YHID" type="string" />
		<property name="name" column="JXMC" type="string" />
		<property name="unit" column="BJDW" type="string" />
		<property name="cDate" column="CDATE" type="timestamp" />
		<property name="pDate" column="BJSJ" type="string" />
		<property name="uDate" column="UDATE" type="timestamp" />
		<property name="data_Source" column="DATA_SOURCE" type="string" />
		<property name="fileName" column="FILENAME" type="string" />
		<property name="webContent" column="WEB_CONTENT" type="string" />
		<property name="webLevel" column="WEB_LEVEL" type="string" />
		<property name="corp_Name" column="CORP_NAME" type="string" />
		<property name="webTitle" column="WEB_TITLE" type="string" />
		<property name="webName" column="WEB_NAME" type="string" />
	</class>
</hibernate-mapping>